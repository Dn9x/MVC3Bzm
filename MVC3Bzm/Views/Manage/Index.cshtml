<!DOCTYPE html>
<html>
<head>
    <title>管理</title>
    <link href="@Url.Content("~/Content/styles/manage.css")" rel="stylesheet" type="text/css" />
    <link rel="icon" href="@Url.Content("~/Content/images/favicon.ico")" type="image/x-icon" />

    <script src="@Url.Content("~/Scripts/head.min.js")" type="text/javascript"></script>
    <script>
        head.js("@Url.Content("~/Scripts/jquery-1.4.2.min.js")")
            .js("@Url.Content("~/Scripts/public.js")");
    </script>
</head>
<body>
    <div id="main">
        <div id="left">
            <table width="200" height="266" border="0" align="center">
              <tr>
                <td height="50" align="center">
                  &nbsp;
                </td>
              </tr>
              <tr>
                <td height="50" align="center">
                  <a href="javascript:PageLoad('Manage/Post');" style="color:#333333; text-decoration:none; cursor:pointer;">發表文章</a>
                </td>
              </tr>
              <tr>
                <td height="50" align="center">
                  <a href="javascript:PageLoad('Manage/Notes');" style="color:#333333; text-decoration:none; cursor:pointer;">瀏覽記錄</a>
                </td>
              </tr>
              <tr>
                <td height="50" align="center">
                  <a href="../Home/Index" target="_blank" style="color:#333333; text-decoration:none; cursor:pointer;">去&nbsp;&nbsp;前&nbsp;&nbsp;端</a>
                </td>
              </tr>
              <tr>
                <td height="50" align="center">
                  <a href="../Manage/LoginOut" style="color:#333333; text-decoration:none; cursor:pointer;">登&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;出</a>
                </td>
              </tr>
           </table>
        </div>
        
        <div id="right">
            <iframe src="" id="Right_Frame" style='border:0px;' width="100%" height="846px"></iframe>
        </div>

        <script type="text/javascript" language="javascript">
            function PageLoad(url) {
                $("#Right_Frame").attr("src", url);
            }
        </script>

    </div>
</body>
</html>

@{  
    Layout = null;  
} 