@using MVC3Bzm.Models.Entity;

@{
    ViewBag.Title = "Notes";
    Layout = null;  
}

<div style="width:100%; height:auto; border:0px solid #CCC; margin:20px auto; font-size:12px;">
    <table width="100%" border="0" align="center" id="Acc_Tab">
      <tbody>
        <tr>
          <td align="center" bgcolor="#33FFFF">编号</td>
          <td align="center" bgcolor="#33FFFF">URL</td>
          <td align="center" bgcolor="#33FFFF">IP</td>
          <td align="center" bgcolor="#33FFFF">DNS</td>
          <td align="center" bgcolor="#33FFFF">浏览器</td>
          <td align="center" bgcolor="#33FFFF">時間</td>
        </tr>
        @{
            List<Access> list = ViewBag.Notes as List<Access>;

            foreach (Access accs in list)
            {
                <tr>
                  <td align='center' bgcolor='#EEEEEE'>@accs.ID</td>
                  <td align='center' bgcolor='#EEEEEE'>@accs.Url</td>
                  <td align='center' bgcolor='#EEEEEE'>@accs.Ip</td>
                  <td align='center' bgcolor='#EEEEEE'>@accs.Dns</td>
                  <td align='center' bgcolor='#EEEEEE'>@accs.Browser</td>
                  <td align='center' bgcolor='#EEEEEE'>@accs.Date</td>
                </tr>
            }
        }
        
      </tbody>
    </table>
</div>
