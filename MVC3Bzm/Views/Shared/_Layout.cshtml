<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/styles/index.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/styles/login.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/styles/bck2.css")" rel="stylesheet" type="text/css" />
    <link rel="icon" href="@Url.Content("~/Content/images/favicon.ico")" type="image/x-icon" />

    <script src="@Url.Content("~/Scripts/jquery-1.4.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/template.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.cookie.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/public.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/skip.js")" type="text/javascript"></script>
</head>
<body>
    <div id="pages">
        <div id="header">
            <input type="checkbox" class="stylec" id="bck1" value="bck1" />
            <input type="checkbox" class="stylec" id="bck2" value="bck2" />
            <input type="checkbox" class="stylec" id="bck3" value="bck3" />
            <a name="Top"></a>
        </div>
        
        <div id="main">
            @RenderBody()
        </div>
        
        <div id="footer"></div>

        <div id="foots">
            <img src="../../Content/images/up_48.png" width="48px" height="48px" />
        </div>
    </div>
</body>
</html>
